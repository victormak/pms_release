<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pms.entity.ContractEntity" table="contract">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="increment"></generator>
		</id>
		<property name ="contract_id" type="java.lang.Integer" column="contract_id" ></property>
		<property name ="apartment_id" type="java.lang.Integer" column="apartment_id" ></property>
		<property name ="tenant_id" type="java.lang.Integer" column="tenant_id" ></property>
		<property name ="contract_frequency" type="java.lang.Integer" column="contract_frequency" ></property>
		<property name ="contract_cycle" type="java.lang.Integer" column="contract_cycle" ></property>
		<property name="contract_starttime" type="string" column="contract_starttime" length="50"></property>
		<property name="contract_endtime" type="string" column="contract_endtime" length="50"></property>
		<property name="contract_month_price" type="string" column="contract_month_price" length="50"></property>
		<property name="contract_deposit" type="string" column="contract_deposit" length="50"></property>
	</class>
</hibernate-mapping>