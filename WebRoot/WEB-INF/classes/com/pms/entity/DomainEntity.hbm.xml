<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pms.entity.DomainEntity" table="domain_info">
		<id name="id" type="java.lang.Integer" column="id">
			<generator class="increment"></generator>
		</id>
		<property name ="domain_id" type="java.lang.Integer" column="domain_id" ></property>
		<property name ="domain_name" type="string" column="domain_name" ></property>
		<property name ="domain_code" type="java.lang.Integer" column="domain_code" ></property>
		<property name ="domain_desc" type="string" column="domain_desc" ></property>
	</class>
</hibernate-mapping>